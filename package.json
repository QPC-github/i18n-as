{
  "name": "rosetta",
  "version": "0.0.1",
  "description": "Gettext implementation for AssemblyScript",
  "main": "index.js",
  "scripts": {
    "test": "npm run build; cd tests/integration; npm test",
    "asbuild:untouched": "asc assembly/index.ts --target debug --transform ./src/transform.ts",
    "asbuild:optimized": "asc assembly/index.ts --target release --transform ./src/transform.ts",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "build": "npx tsc --p tsconfig.json"
  },
  "author": "Shopify",
  "license": "ISC",
  "devDependencies": {
    "@as-pect/cli": "^4.0.0",
    "@assemblyscript/loader": "^0.14.13",
    "@types/node": "^13.9.1",
    "assemblyscript": "^0.14.0",
    "assemblyscript-json": "^1.0.0",
    "pkg-dir": "5.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "visitor-as": "0.3.2"
  }
}
